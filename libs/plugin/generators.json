{
  "$schema": "http://json-schema.org/schema",
  "name": "plugin",
  "version": "3.0.0-rc.1",
  "generators": {
    "migrate-from-cb3": {
      "factory": "./src/generators/migrate-from-cb3/index",
      "schema": "./src/generators/migrate-from-cb3/schema.json",
      "description": "helpers to migrate from cb3 to prizm"
    },
    "remove-prizm-deprecated-v1-2": {
      "factory": "./src/generators/remove-prizm-deprecated-v1-2/index",
      "schema": "./src/generators/remove-prizm-deprecated-v1-2/schema.json",
      "description": "helpers to remove deprecated prizm"
    },
    "update-version": {
      "factory": "./src/generators/update-version/index",
      "schema": "./src/generators/update-version/schema.json",
      "description": "helpers to update version for publishable version"
    },
    "update-structure": {
      "factory": "./src/generators/update-structure/index",
      "schema": "./src/generators/update-structure/schema.json",
      "description": "helpers to update structure with replace extension"
    }
  }
}
